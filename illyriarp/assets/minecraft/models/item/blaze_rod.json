{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "minecraft:item/blaze_rod"
    },
    "cases": [
      {
        "when": "rules",
        "model": {
          "type": "model",
          "model": "custom:item/faq/rules"
        }
      },
      {
        "when": "dimensions",
        "model": {
          "type": "model",
          "model": "custom:item/faq/dimensions"
        }
      },
      {
        "when": "settings",
        "model": {
          "type": "model",
          "model": "custom:item/faq/settings"
        }
      },
      {
        "when": "dim_overworld",
        "model": {
          "type": "model",
          "model": "custom:item/dimensions/dim_overworld"
        }
      },
      {
        "when": "dim_underworld",
        "model": {
          "type": "model",
          "model": "custom:item/dimensions/dim_underworld"
        }
      },
      {
        "when": "dim_endworld",
        "model": {
          "type": "model",
          "model": "custom:item/dimensions/dim_endworld"
        }
      }
    ]
  }
}
