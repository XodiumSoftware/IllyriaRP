{
  "model": {
    "type": "select",
    "property": "context_entity_type",
    "fallback": {
      "type": "select",
      "property": "custom_model_data",
      "fallback": {
        "type": "model",
        "model": "item/stone_sword"
      },
      "cases": [
        {
          "when": "wither",
          "model": {
            "type": "model",
            "model": "custom:item/skins/wither_sword"
          }
        },
        {
          "when": "elder_guardian",
          "model": {
            "type": "model",
            "model": "custom:item/skins/elder_guardian"
          }
        },
        {
          "when": "warden",
          "model": {
            "type": "model",
            "model": "custom:item/skins/warden"
          }
        },
        {
          "when": "ender_dragon",
          "model": {
            "type": "model",
            "model": "custom:item/skins/ender_dragon"
          }
        }
      ]
    },
    "cases": [
      {
        "when": "wither_skeleton",
        "model": {
          "type": "model",
          "model": "item/excalibur/stone_sword_wither_skeleton"
        }
      }
    ]
  }
}
