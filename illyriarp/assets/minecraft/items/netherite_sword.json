{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/netherite_sword"
    },
    "cases": [
      {
        "when": "wither",
        "model": {
          "type": "model",
          "model": "custom:item/wither_sword"
        }
      },
      {
        "when": "elder_guardian",
        "model": {
          "type": "model",
          "model": "custom:item/elder_guardian"
        }
      },
      {
        "when": "warden",
        "model": {
          "type": "model",
          "model": "custom:item/warden"
        }
      },
      {
        "when": "ender_dragon",
        "model": {
          "type": "model",
          "model": "custom:item/ender_dragon"
        }
      }
    ]
  }
}
